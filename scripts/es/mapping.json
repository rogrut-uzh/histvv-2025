        "type": "text",
        "analyzer": "de_analyzer"
      },
      "zusatz": {
        "type": "text",
        "analyzer": "de_analyzer"
      },
      "zeit": {
        "type": "keyword"
      },
      "wochenstunden": {
        "type": "keyword"
      },
      "ort": {
        "type": "text",
        "fields": {
          "keyword": {
            "type": "keyword"
          }
        }
      },
      "fak": {
        "type": "keyword",
