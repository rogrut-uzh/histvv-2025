{
  "settings": {
    "number_of_shards": 1,
    "number_of_replicas": 1,
    "analysis": {
      "analyzer": {
        "de_analyzer": {
          "type": "standard",
          "stopwords": "_german_"
        },
        "name_analyzer": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "asciifolding"
          ]
        }
      },
      "normalizer": {
        "lowercase_normalizer": {
          "type": "custom",
          "filter": ["lowercase", "asciifolding"]
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "typ": {
        "type": "keyword"
      },
      "id_semester": {
        "type": "keyword"
      },
      "vorlesungsnummer": {
        "type": "keyword"
      },
      "id_veranstaltung": {
        "type": "keyword"
      },
      "dozenten": {
        "type": "nested",
        "properties": {
          "id_dozent": {
            "type": "keyword"
          },
          "nachname": {
            "type": "text",
            "analyzer": "name_analyzer",
            "fields": {
              "keyword": {
                "type": "keyword"
              }
            }
          },
          "vorname": {
            "type": "text",
            "analyzer": "name_analyzer",
            "fields": {
              "keyword": {
                "type": "keyword"
              }
            }
          },
          "grad": {
            "type": "keyword"
          },
          "funktion": {
            "type": "keyword"
          }
        }
      },
      "thema": {
